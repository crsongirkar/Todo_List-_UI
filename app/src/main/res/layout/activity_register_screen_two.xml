<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="registerScreenTwoVM" type="com.chinmaysapplication.app.modules.registerscreentwo.data.viewmodel.RegisterScreenTwoVM"/>
    </data>
    <LinearLayout android:id="@+id/linearRegisterScreen"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_901">
        <ImageView android:id="@+id/imageLocation"
            android:layout_gravity="start"
            android:layout_height="@dimen/_17pxh"
            android:layout_marginEnd="@dimen/_31pxh"
            android:layout_marginStart="@dimen/_31pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="@dimen/_8pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_location"
            tools:ignore="ContentDescription" tools:src="@drawable/img_location"/>
        <TextView android:ellipsize="end" android:id="@+id/txtRegister"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_21pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{registerScreenTwoVM.registerScreenTwoModel.txtRegister}"
            style="@style/txtLatobold32" tools:text="@string/lbl_register"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearUsername"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_21pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylecornerRadius">
            <TextView android:ellipsize="end"
                android:id="@+id/txtUsername"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{registerScreenTwoVM.registerScreenTwoModel.txtUsername}"
                style="@style/txtLatoregular16" tools:text="@string/lbl_username"/>
            <EditText
                android:background="@drawable/rectangle_bg_gray_902_border_gray_500_radius_4"
                android:ellipsize="end"
                android:hint="@string/lbl_mahsa_esfehany"
                android:id="@+id/etUsernameFuild"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_12pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_12pxh"
                android:paddingTop="@dimen/_15pxv"
                android:singleLine="true"
                android:text="@={registerScreenTwoVM.registerScreenTwoModel.etUsernameFuildValue}"
                android:textColorHint="@color/white_A700_dd"
                style="@style/etSolidRoundedOutline_2" tools:ignore="Autofill"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearPassword"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_27pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylecornerRadius">
            <TextView android:ellipsize="end"
                android:id="@+id/txtPassword"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{registerScreenTwoVM.registerScreenTwoModel.txtPassword}"
                style="@style/txtLatoregular16" tools:text="@string/lbl_password"/>
            <ImageView android:id="@+id/imagePasswordFuild"
                android:layout_height="@dimen/_48pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_passwordfuild"
                tools:ignore="ContentDescription" tools:src="@drawable/img_passwordfuild"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearPassword1"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_27pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylecornerRadius">
            <TextView android:ellipsize="end"
                android:id="@+id/txtConfirmPasswor"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{registerScreenTwoVM.registerScreenTwoModel.txtConfirmPasswor}"
                style="@style/txtLatoregular16" tools:text="@string/msg_confirm_passwor"/>
            <ImageView android:id="@+id/imagePasswordFuildOne"
                android:layout_height="@dimen/_48pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_passwordfuild"
                tools:ignore="ContentDescription" tools:src="@drawable/img_passwordfuild"/>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnRegisterOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_14pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_14pxv" android:singleLine="true"
            android:text="@string/lbl_register"
            android:textAllCaps="false" style="@style/btnSolidRounded_2" tools:text="@string/lbl_register"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearDivider"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_21pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <View android:background="@color/gray_500"
                android:id="@+id/lineVectorOne"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginBottom="@dimen/_8pxv"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_weight="0.48" android:layout_width="0dp"/>
            <TextView android:ellipsize="end" android:id="@+id/txtOr"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_weight="0.05" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{registerScreenTwoVM.registerScreenTwoModel.txtOr}"
                style="@style/txtLatoregular16_6" tools:text="@string/lbl_or"/>
            <View android:background="@color/gray_500"
                android:id="@+id/lineVectorTwo"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginBottom="@dimen/_8pxv"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_weight="0.47" android:layout_width="0dp"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearGoogle"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylecornerRadius_2">
            <ImageView android:id="@+id/imageGoogle"
                android:layout_height="@dimen/_21pxh"
                android:layout_marginBottom="@dimen/_13pxv"
                android:layout_marginStart="@dimen/_74pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="@dimen/_21pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_google"
                tools:ignore="ContentDescription" tools:src="@drawable/img_google"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtRegisterwithG"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_marginEnd="@dimen/_72pxh"
                android:layout_marginStart="@dimen/_11pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_weight="0.87" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{registerScreenTwoVM.registerScreenTwoModel.txtRegisterwithG}"
                style="@style/txtLatoregular16" tools:text="@string/msg_register_with_g"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearApple"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylecornerRadius_2">
            <ImageView android:id="@+id/imageRefresh"
                android:layout_height="@dimen/_21pxh"
                android:layout_marginBottom="@dimen/_13pxv"
                android:layout_marginStart="@dimen/_80pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="@dimen/_21pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_refresh"
                tools:ignore="ContentDescription" tools:src="@drawable/img_refresh"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtRegisterwithA"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_marginEnd="@dimen/_79pxh"
                android:layout_marginStart="@dimen/_11pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_weight="0.86" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{registerScreenTwoVM.registerScreenTwoModel.txtRegisterwithA}"
                style="@style/txtLatoregular16" tools:text="@string/msg_register_with_a"/>
        </LinearLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtConfirmation"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_49pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{registerScreenTwoVM.registerScreenTwoModel.txtConfirmation}"
            style="@style/txtLatoregular12" tools:text="@string/msg_already_have_an"/>
    </LinearLayout>
</layout>
