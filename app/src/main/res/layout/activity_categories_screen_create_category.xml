<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="categoriesScreenCreateCategoryVM" type="com.chinmaysapplication.app.modules.categoriesscreencreatecategory.data.viewmodel.CategoriesScreenCreateCategoryVM"/>
    </data>
    <FrameLayout android:id="@+id/frameCategoriesscre"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_901">
        <LinearLayout android:id="@+id/linearColumncreatenewcate"
            android:layout_gravity="top|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_25pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtCreatenewcate"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:singleLine="true"
                android:text="@{categoriesScreenCreateCategoryVM.categoriesScreenCreateCategoryModel.txtCreatenewcate}"
                style="@style/txtLatobold20" tools:text="@string/msg_create_new_cate"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtCategoryname"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{categoriesScreenCreateCategoryVM.categoriesScreenCreateCategoryModel.txtCategoryname}"
                style="@style/txtLatoregular16" tools:text="@string/lbl_category_name"/>
            <EditText
                android:background="@drawable/rectangle_bg_gray_902_border_white_a700_radius_4"
                android:ellipsize="end"
                android:hint="@string/lbl_family"
                android:id="@+id/etSearchBar" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_12pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_12pxh"
                android:paddingTop="@dimen/_15pxv"
                android:singleLine="true"
                android:text="@={categoriesScreenCreateCategoryVM.categoriesScreenCreateCategoryModel.etSearchBarValue}"
                android:textColorHint="@color/white_A700_dd"
                style="@style/etSolidRoundedOutline_4" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtCategoryicon"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{categoriesScreenCreateCategoryVM.categoriesScreenCreateCategoryModel.txtCategoryicon}"
                style="@style/txtLatoregular16" tools:text="@string/lbl_category_icon"/>
            <ImageButton
                android:background="@drawable/rectangle_bg_white_a700_35_radius_6"
                android:id="@+id/btnHome"
                android:layout_height="@dimen/_42pxh"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="@dimen/_42pxh"
                android:paddingHorizontal="@dimen/_10pxh"
                android:paddingVertical="@dimen/_10pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_home_white_a700" tools:ignore="ContentDescription"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtCategorycolor"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{categoriesScreenCreateCategoryVM.categoriesScreenCreateCategoryModel.txtCategorycolor}"
                style="@style/txtLatoregular16" tools:text="@string/msg_category_color"/>
        </LinearLayout>
        <HorizontalScrollView android:fillViewport="true"
            android:id="@+id/horizontalScrollHorizontalscrollcheckmark"
            android:layout_gravity="top|end"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_299pxv"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_299pxv"
            android:layout_width="match_parent" android:scrollbars="none">
            <LinearLayout android:id="@+id/linearRowellipsetwentythree"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowcheckmark"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.7"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_lime_600_radius_18"
                        android:id="@+id/btnCheckmark"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:paddingHorizontal="@dimen/_9pxh"
                        android:paddingVertical="@dimen/_9pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_checkmark_white_a700" tools:ignore="ContentDescription"/>
                    <View
                        android:background="@drawable/rectangle_bg_light_green_600_radius_18"
                        android:id="@+id/viewEllipseSeventeen"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_marginStart="@dimen/_12pxh" android:layout_width="@dimen/_36pxh"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageEllipseTwentyThree"
                    android:layout_height="@dimen/_36pxh"
                    android:layout_marginStart="@dimen/_252pxh"
                    android:layout_width="@dimen/_36pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_ellipse23_2"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse23_2"/>
            </LinearLayout>
        </HorizontalScrollView>
        <LinearLayout android:id="@+id/linearRowellipseeighteen"
            android:layout_gravity="top|start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_299pxv"
            android:layout_marginEnd="@dimen/_120pxh"
            android:layout_marginStart="@dimen/_120pxh"
            android:layout_marginTop="@dimen/_299pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <View
                android:background="@drawable/rectangle_bg_teal_300_radius_18"
                android:id="@+id/viewEllipseEighteen"
                android:layout_height="@dimen/_36pxh" android:layout_width="@dimen/_36pxh"/>
            <View
                android:background="@drawable/rectangle_bg_indigo_400_radius_18"
                android:id="@+id/viewEllipseNineteen"
                android:layout_height="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_12pxh" android:layout_width="@dimen/_36pxh"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowellipsetwenty"
            android:layout_gravity="top|end"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_299pxv"
            android:layout_marginEnd="@dimen/_75pxh"
            android:layout_marginStart="@dimen/_75pxh"
            android:layout_marginTop="@dimen/_299pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <View
                android:background="@drawable/rectangle_bg_teal_301_radius_18"
                android:id="@+id/viewEllipseTwenty"
                android:layout_height="@dimen/_36pxh" android:layout_width="@dimen/_36pxh"/>
            <View
                android:background="@drawable/rectangle_bg_deep_orange_400_radius_18"
                android:id="@+id/viewEllipseTwentyOne"
                android:layout_height="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_12pxh" android:layout_width="@dimen/_36pxh"/>
        </LinearLayout>
        <View
            android:background="@drawable/rectangle_bg_purple_400_radius_18"
            android:id="@+id/viewEllipseTwentyTwo"
            android:layout_gravity="top|end"
            android:layout_height="@dimen/_36pxh"
            android:layout_marginBottom="@dimen/_299pxv"
            android:layout_marginEnd="@dimen/_27pxh"
            android:layout_marginStart="@dimen/_27pxh"
            android:layout_marginTop="@dimen/_299pxv" android:layout_width="@dimen/_36pxh"/>
        <LinearLayout android:id="@+id/linearRowcancel"
            android:layout_gravity="bottom|end"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_60pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_60pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearCreateCategory"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_13pxv"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_weight="0.24" android:layout_width="0dp"
                android:orientation="vertical" style="@style/groupStylecornerRadius">
                <TextView android:ellipsize="end"
                    android:gravity="start" android:id="@+id/txtCancel"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{categoriesScreenCreateCategoryVM.categoriesScreenCreateCategoryModel.txtCancel}"
                    style="@style/txtLatoregular16_2" tools:text="@string/lbl_cancel"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnCreateCategoryOne"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_73pxh"
                android:layout_weight="0.76" android:layout_width="0dp"
                android:paddingBottom="@dimen/_14pxv"
                android:paddingEnd="@dimen/_18pxh"
                android:paddingStart="@dimen/_18pxh"
                android:paddingTop="@dimen/_14pxv"
                android:singleLine="true"
                android:text="@string/lbl_create_category"
                android:textAllCaps="false"
                style="@style/btnSolidRounded" tools:text="@string/lbl_create_category"/>
        </LinearLayout>
    </FrameLayout>
</layout>
