<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="onboadingOneVM" type="com.chinmaysapplication.app.modules.onboadingone.data.viewmodel.OnboadingOneVM"/>
    </data>
    <FrameLayout android:id="@+id/frameOnboadingOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_901">
        <LinearLayout android:id="@+id/linearColumnskip"
            android:layout_gravity="top|center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_16pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:id="@+id/frameStackskip"
                android:layout_gravity="start"
                android:layout_height="@dimen/_310pxv"
                android:layout_marginEnd="@dimen/_10pxh" android:layout_width="@dimen/_299pxh">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSkip"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{onboadingOneVM.onboadingOneModel.txtSkip}"
                    android:textAllCaps="true"
                    style="@style/txtLatoregular16_3" tools:text="@string/lbl_skip"/>
                <ImageView android:id="@+id/imageGroup183"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_296pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_271pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group183"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group183"/>
            </FrameLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowrectangle101"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_42pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <View
                    android:background="@drawable/rectangle_bg_gray_400_radius_2"
                    android:id="@+id/viewRectangle101"
                    android:layout_height="@dimen/_4pxv" android:layout_width="@dimen/_26pxh"/>
                <View
                    android:background="@drawable/rectangle_bg_white_a700_dd_radius_2"
                    android:id="@+id/viewRectangle100"
                    android:layout_height="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_8pxh" android:layout_width="@dimen/_26pxh"/>
                <View
                    android:background="@drawable/rectangle_bg_gray_400_radius_2"
                    android:id="@+id/viewRectangle102"
                    android:layout_height="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_8pxh" android:layout_width="@dimen/_26pxh"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtCreatedailyro"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_19pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_52pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{onboadingOneVM.onboadingOneModel.txtCreatedailyro}"
                style="@style/txtLatobold32" tools:text="@string/msg_create_daily_ro"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtLanguage"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_40pxv"
                android:layout_width="match_parent"
                android:text="@{onboadingOneVM.onboadingOneModel.txtLanguage}"
                style="@style/txtLatoregular16" tools:text="@string/msg_in_uptodo_you"/>
            <LinearLayout android:id="@+id/linearRowback"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_106pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtBack"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_weight="0.32"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{onboadingOneVM.onboadingOneModel.txtBack}"
                    android:textAllCaps="true"
                    style="@style/txtLatoregular16_3" tools:text="@string/lbl_back"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnNext"
                    android:layout_height="@dimen/_48pxv"
                    android:layout_marginStart="@dimen/_193pxh"
                    android:layout_width="@dimen/_90pxh"
                    android:paddingBottom="@dimen/_14pxv"
                    android:paddingEnd="@dimen/_24pxh"
                    android:paddingStart="@dimen/_24pxh"
                    android:paddingTop="@dimen/_14pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_next"
                    android:textAllCaps="true"
                    style="@style/btnSolidRounded_1" tools:text="@string/lbl_next"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
