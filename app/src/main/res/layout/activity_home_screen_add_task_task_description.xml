<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="homeScreenAddTaskTaskDescriptionVM" type="com.chinmaysapplication.app.modules.homescreenaddtasktaskdescription.data.viewmodel.HomeScreenAddTaskTaskDescriptionVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackbottonnavigati"
        android:layout_height="match_parent" android:layout_width="match_parent">
        <LinearLayout android:id="@+id/linearColumnbottonnavigati"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameHomeScreenA"
                android:layout_height="match_parent"
                android:layout_width="match_parent" style="@style/groupStylegray_901">
                <View android:id="@+id/viewBottonNavigati"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_132pxv"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_375pxh"/>
                <LinearLayout android:id="@+id/linearColumn"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyleblack_900_bc">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnaddtask"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_322pxv"
                        android:layout_marginTop="@dimen/_218pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylebluegray_1">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtAddTask"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_25pxh"
                            android:layout_marginStart="@dimen/_25pxh"
                            android:layout_marginTop="@dimen/_28pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{homeScreenAddTaskTaskDescriptionVM.homeScreenAddTaskTaskDescriptionModel.txtAddTask}"
                            style="@style/txtLatobold20" tools:text="@string/lbl_add_task"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDomathhomewor"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_24pxh"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_26pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{homeScreenAddTaskTaskDescriptionVM.homeScreenAddTaskTaskDescriptionModel.txtDomathhomewor}"
                            style="@style/txtLatoregular18" tools:text="@string/msg_do_math_homewor2"/>
                        <EditText
                            android:background="@drawable/rectangle_border_gray_500_radius_4"
                            android:ellipsize="end"
                            android:hint="@string/msg_do_chapter_2_to"
                            android:id="@+id/etFrame164"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_24pxh"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_23pxv"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_9pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_11pxv"
                            android:singleLine="true"
                            android:text="@={homeScreenAddTaskTaskDescriptionVM.homeScreenAddTaskTaskDescriptionModel.etFrame164Value}"
                            android:textColorHint="@color/white_A700_dd"
                            style="@style/etRoundedOutline" tools:ignore="Autofill"/>
                        <LinearLayout android:id="@+id/linearRowreply"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_18pxv"
                            android:layout_marginEnd="@dimen/_24pxh"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_20pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <LinearLayout
                                android:id="@+id/linearRowclock"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.87"
                                android:layout_width="0dp" android:orientation="horizontal">
                                <ImageView android:id="@+id/imageClock"
                                    android:layout_height="@dimen/_21pxh"
                                    android:layout_width="@dimen/_19pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_clock"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_clock"/>
                                <ImageView
                                    android:id="@+id/imageLocation"
                                    android:layout_height="@dimen/_19pxh"
                                    android:layout_marginBottom="@dimen/_1pxv"
                                    android:layout_marginStart="@dimen/_36pxh"
                                    android:layout_marginTop="@dimen/_1pxv"
                                    android:layout_width="@dimen/_19pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_location_white_a700"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_location_white_a700"/>
                                <ImageView
                                    android:id="@+id/imageComputer"
                                    android:layout_height="@dimen/_21pxh"
                                    android:layout_marginStart="@dimen/_38pxh"
                                    android:layout_width="@dimen/_15pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_computer"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_computer"/>
                            </LinearLayout>
                            <ImageView android:id="@+id/imageReply"
                                android:layout_height="@dimen/_19pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_173pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_19pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_reply"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_reply"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_33pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowmenu"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageMenu"
                        android:layout_height="@dimen/_11pxh"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_width="@dimen/_19pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_menu"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_menu"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtIndex"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginStart="@dimen/_110pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_weight="0.43"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_5pxh"
                        android:singleLine="true"
                        android:text="@{homeScreenAddTaskTaskDescriptionVM.homeScreenAddTaskTaskDescriptionModel.txtIndex}"
                        style="@style/txtLatoregular20" tools:text="@string/lbl_index"/>
                    <ImageView android:id="@+id/imageEllipseThirteen"
                        android:layout_height="@dimen/_42pxh"
                        android:layout_marginStart="@dimen/_97pxh"
                        android:layout_width="@dimen/_42pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_ellipse13}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse13"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <FrameLayout android:gravity="start"
                android:id="@+id/frameBottombar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" style="@style/groupStylebluegray_900">
                <LinearLayout android:id="@+id/linearRowsignal"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_38pxv"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearColumnsignal"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageSignal"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_signal"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_signal"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtIndexOne"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_12pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{homeScreenAddTaskTaskDescriptionVM.homeScreenAddTaskTaskDescriptionModel.txtIndexOne}"
                            style="@style/txtLatoregular12" tools:text="@string/lbl_index"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearColumncalendar"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageCalendar"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_21pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_19pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_calendar"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_calendar"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtCalendarOne"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{homeScreenAddTaskTaskDescriptionVM.homeScreenAddTaskTaskDescriptionModel.txtCalendarOne}"
                            style="@style/txtLatoregular12_3" tools:text="@string/lbl_calendar"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearColumnvector"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <FrameLayout android:id="@+id/frameClock"
                            android:layout_height="@dimen/_21pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_21pxh" style="@style/groupStylewhite_A700_f9cornerRadius">
                            <ImageView android:id="@+id/imageVector"
                                android:layout_gravity="center"
                                android:layout_height="@dimen/_9pxh"
                                android:layout_marginBottom="@dimen/_10pxv"
                                android:layout_marginTop="@dimen/_5pxv"
                                android:layout_width="@dimen/_5pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_vector_white_a700"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_white_a700"/>
                        </FrameLayout>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtFocuse"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{homeScreenAddTaskTaskDescriptionVM.homeScreenAddTaskTaskDescriptionModel.txtFocuse}"
                            style="@style/txtLatoregular12_3" tools:text="@string/lbl_focuse"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearColumnuser"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageUser"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_21pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_user"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtProfile"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{homeScreenAddTaskTaskDescriptionVM.homeScreenAddTaskTaskDescriptionModel.txtProfile}"
                            style="@style/txtLatoregular12_3" tools:text="@string/lbl_profile"/>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:backgroundTint="@color/indigo_300"
            android:id="@+id/floatingBtnFloatingactionbutton"
            android:layout_gravity="bottom|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_68pxv"
            android:layout_marginEnd="@dimen/_155pxh"
            android:layout_marginStart="@dimen/_155pxh"
            android:layout_marginTop="@dimen/_68pxv"
            android:layout_width="wrap_content"
            app:fabCustomSize="@dimen/_64pxh"
            app:srcCompat="@drawable/img_plus" app:tint="@color/white_A700"/>
    </FrameLayout>
</layout>
