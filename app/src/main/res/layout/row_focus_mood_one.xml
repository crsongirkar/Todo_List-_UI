<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="focusMoodOneRowModel" type="com.chinmaysapplication.app.modules.focusmoodone.data.model.FocusMoodOneRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearAppBox"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylegray_800cornerRadius">
        <ImageView android:id="@+id/imageInstagram"
            android:layout_height="@dimen/_28pxh"
            android:layout_marginBottom="@dimen/_14pxv"
            android:layout_marginStart="@dimen/_14pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_width="@dimen/_28pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_instagram"
            tools:ignore="ContentDescription" tools:src="@drawable/img_instagram"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumninstagramOne"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_7pxv"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_weight="0.8" android:layout_width="0dp" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtInstagramOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{focusMoodOneRowModel.txtInstagramOne}"
                style="@style/txtLatoregular14_2" tools:text="@string/lbl_instagram"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtYouspent4hon"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{focusMoodOneRowModel.txtYouspent4hon}"
                style="@style/txtLatoregular12_1" tools:text="@string/msg_you_spent_4h_on"/>
        </LinearLayout>
        <View android:background="@color/gray_500"
            android:id="@+id/lineVectorFour"
            android:layout_height="@dimen/_41pxv"
            android:layout_marginBottom="@dimen/_7pxv"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_marginTop="@dimen/_8pxv"
            android:layout_weight="0.01" android:layout_width="0dp"/>
        <ImageView android:id="@+id/imageWarning"
            android:layout_height="@dimen/_21pxh"
            android:layout_marginBottom="@dimen/_17pxv"
            android:layout_marginEnd="@dimen/_17pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="@dimen/_21pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_warning_white_a700"
            tools:ignore="ContentDescription" tools:src="@drawable/img_warning_white_a700"/>
    </LinearLayout>
</layout>
